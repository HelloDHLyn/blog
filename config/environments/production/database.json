{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "bookshelf",
      "settings": {
        "client": "postgres",
        "host": "${process.env.DB_HOST || '127.0.0.1'}",
        "port": "${process.env.DB_PORT}",
        "database": "${process.env.DB_NAME}",
        "username": "${process.env.DB_USERNAME}",
        "password": "${process.env.DB_PASSWORD}"
      },
      "options": {}
    }
  }
}
